<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.lpuniv.Dayoung.dao">
    <select id="list" resultType="com.project.lpuniv.Dayoung.login.Dto.LoginDto">
        select * from user
    </select>

    <select id="login" resultType="com.project.lpuniv.Dayoung.login.Dto.LoginDto">
        select user_passwd from user
        where user_loginId=#{user_loginId}
    </select>
    <select id="">

    </select>
</mapper>
